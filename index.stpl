                        option.className = 'language-option';
                        option.textContent = name;
                        option.onclick = () => addLanguage(name, code);
                        languageDropdown.appendChild(option);
                    }
                });
        });

        document.addEventListener('click', (e) => {
            if (!languageSearch.contains(e.target)) {
                languageDropdown.classList.add('hidden');
            }
        });

        function addLanguage(name, code) {
            selectedLanguages.push(code);
            updateSelectedLanguagesUI();
            languageDropdown.classList.add('hidden');
            languageSearch.value = '';
            populate(toReveal)
        }

        function removeLanguage(code) {
            selectedLanguages.splice(selectedLanguages.indexOf(code), 1);
            updateSelectedLanguagesUI();
            populate(toReveal)
        }

        function updateSelectedLanguagesUI() {
            selectedLanguagesContainer.innerHTML = '';
            selectedLanguages.forEach(name => {
                const tag = document.createElement('span');
                tag.className = 'language-tag';
                tag.innerHTML = `
                    ${languageCodes[name + '_words']}
                    <button onclick='removeLanguage(\"${name}\")'>×</button>
                `;
                selectedLanguagesContainer.appendChild(tag);
            });
        }

        async function fetchRelatedWords(word) {
            return fetch(`/next/${word}`)
                .then(response => response.json())
                .then(data => {
                    state.data = data;
                });
        }

        populate(toReveal).then(showRelatedWords); addLanguage("Czech", "cs");
    </script>
</body>

</html>
